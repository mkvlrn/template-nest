{
  "name": "template-nest",
  "description": "A template for NestJS projects",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": "Mike Valeriano <mkvlrn@proton.me>",
  "repository": {
    "type": "git",
    "url": "git@github.com:mkvlrn/template-nest"
  },
  "keywords": [
    "nest",
    "esm",
    "template"
  ],
  "scripts": {
    "build": "nest build",
    "postbuild": "node --import @swc-node/register/esm-register scripts/post-build.ts",
    "dev": "node --import @swc-node/register/esm-register --watch src/main.ts",
    "start": "node dist/main.js",
    "test": "vitest --run",
    "test:cov": "vitest run --coverage",
    "lint": "eslint --fix src/**/*.ts test/**/*.ts",
    "format": "prettier --write src test",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@mkvlrn/config": "^1.0.3",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.4.1-nightly.20240914",
    "@swc/core": "^1.7.40",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.4",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3",
    "vite-plugin-swc-transform": "^1.0.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.6",
    "@nestjs/core": "^10.4.6",
    "@nestjs/platform-express": "^10.4.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  }
}
