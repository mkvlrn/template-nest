{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": {
      "type": "swc",
      "options": {
        "outDir": "./build"
      }
    },
    "deleteOutDir": true
  },
  "generateOptions": {
    "spec": false
  }
}
